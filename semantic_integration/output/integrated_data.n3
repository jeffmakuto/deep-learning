<http://university.edu/ontology> a owl:Ontology ;
    rdfs:label "University Academic Ontology" ;
    rdfs:comment "Domain ontology for university academic system integration" ;
    owl:versionInfo "1.0" .

uni:budget a owl:DatatypeProperty ;
    rdfs:label "budget" ;
    rdfs:domain uni:Department ;
    rdfs:range xsd:integer .

uni:building a owl:DatatypeProperty ;
    rdfs:label "building" ;
    rdfs:domain uni:Department ;
    rdfs:range xsd:string .

uni:courseCode a owl:DatatypeProperty ;
    rdfs:label "course code" ;
    rdfs:domain uni:Course ;
    rdfs:range xsd:string .

uni:courseName a owl:DatatypeProperty ;
    rdfs:label "course name" ;
    rdfs:domain uni:Course ;
    rdfs:range xsd:string .

uni:credits a owl:DatatypeProperty ;
    rdfs:label "credits" ;
    rdfs:domain uni:Course ;
    rdfs:range xsd:integer .

uni:dateOfBirth a owl:DatatypeProperty ;
    rdfs:label "date of birth" ;
    rdfs:domain uni:Student ;
    rdfs:range xsd:date .

uni:departmentID a owl:DatatypeProperty ;
    rdfs:label "department ID" ;
    rdfs:domain uni:Department ;
    rdfs:range xsd:string .

uni:departmentName a owl:DatatypeProperty ;
    rdfs:label "department name" ;
    rdfs:domain uni:Department ;
    rdfs:range xsd:string .

uni:email a owl:DatatypeProperty ;
    rdfs:label "email" ;
    rdfs:domain uni:Person ;
    rdfs:range xsd:string .

uni:enrolledIn a owl:ObjectProperty ;
    rdfs:label "enrolled in" ;
    rdfs:comment "Links a student to a course they are enrolled in" ;
    rdfs:domain uni:Student ;
    rdfs:range uni:Course .

uni:enrollmentFor a owl:ObjectProperty ;
    rdfs:label "enrollment for" ;
    rdfs:comment "Links an enrollment to its course" ;
    rdfs:domain uni:Enrollment ;
    rdfs:range uni:Course .

uni:enrollmentYear a owl:DatatypeProperty ;
    rdfs:label "enrollment year" ;
    rdfs:domain uni:Student ;
    rdfs:range xsd:gYear .

uni:establishedYear a owl:DatatypeProperty ;
    rdfs:label "established year" ;
    rdfs:domain uni:Department ;
    rdfs:range xsd:gYear .

uni:facultyCount a owl:DatatypeProperty ;
    rdfs:label "faculty count" ;
    rdfs:domain uni:Department ;
    rdfs:range xsd:integer .

uni:firstName a owl:DatatypeProperty ;
    rdfs:label "first name" ;
    rdfs:domain uni:Person ;
    rdfs:range xsd:string .

uni:gpa a owl:DatatypeProperty ;
    rdfs:label "GPA" ;
    rdfs:domain uni:Student ;
    rdfs:range xsd:decimal .

uni:grade a owl:DatatypeProperty ;
    rdfs:label "grade" ;
    rdfs:domain uni:Enrollment ;
    rdfs:range xsd:string .

uni:hasEnrollment a owl:ObjectProperty ;
    rdfs:label "has enrollment" ;
    rdfs:comment "Links a student to their enrollment record" ;
    rdfs:domain uni:Student ;
    rdfs:range uni:Enrollment .

uni:headedBy a owl:ObjectProperty ;
    rdfs:label "headed by" ;
    rdfs:comment "Links a department to its head" ;
    rdfs:domain uni:Department ;
    rdfs:range uni:Instructor .

uni:instructorID a owl:DatatypeProperty ;
    rdfs:label "instructor ID" ;
    rdfs:domain uni:Instructor ;
    rdfs:range xsd:string .

uni:lastName a owl:DatatypeProperty ;
    rdfs:label "last name" ;
    rdfs:domain uni:Person ;
    rdfs:range xsd:string .

uni:majorIn a owl:ObjectProperty ;
    rdfs:label "major in" ;
    rdfs:comment "Links a student to their major department" ;
    rdfs:domain uni:Student ;
    rdfs:range uni:Department .

uni:offeredBy a owl:ObjectProperty ;
    rdfs:label "offered by" ;
    rdfs:comment "Links a course to the department offering it" ;
    rdfs:domain uni:Course ;
    rdfs:range uni:Department .

uni:semester a owl:DatatypeProperty ;
    rdfs:label "semester" ;
    rdfs:domain uni:Enrollment ;
    rdfs:range xsd:string .

uni:studentID a owl:DatatypeProperty ;
    rdfs:label "student ID" ;
    rdfs:domain uni:Student ;
    rdfs:range xsd:string .

uni:teaches a owl:ObjectProperty ;
    rdfs:label "teaches" ;
    rdfs:comment "Links an instructor to a course they teach" ;
    rdfs:domain uni:Instructor ;
    rdfs:range uni:Course .

